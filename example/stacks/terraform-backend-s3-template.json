{
  "terraform": {
    "backend": {
      "s3": {
        "acl": "bucket-owner-full-control",
        "bucket": "$${namespace}-$${environment}-$${stage}-tfstate",
        "dynamodb_table": "$${namespace}-$${environment}-$${stage}-tfstate-lock",
        "encrypt": true,
        "key": "terraform.tfstate",
        "region": "us-east-2",
        "role_arn": "$${role_arn}",
        "workspace_key_prefix": "$${workspace_key_prefix}"
      }
    }
  }
}
