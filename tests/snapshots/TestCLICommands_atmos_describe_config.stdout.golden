      "kubeconfig_path": "",
      "helm_aws_profile_pattern": "",
      "cluster_name_pattern": "",
      "command": ""
    }
  },
  "stacks": {
    "base_path": "stacks",
    "included_paths": [
      "deploy/**/*"
    ],
    "excluded_paths": [
      "**/_defaults.yaml"
    ],
    "name_pattern": "{stage}",
    "name_template": ""
