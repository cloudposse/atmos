atmos_cli_config:
  base_path: .
  components:
    terraform:
      base_path: components/terraform
      apply_auto_approve: false
      append_user_agent: Atmos/test (Cloud Posse; +https://atmos.tools)
      deploy_run_init: true
      init_run_reconfigure: true
      auto_generate_backend_file: true
      auto_generate_files: false
      command: terraform
      shell:
        prompt: ""
      init:
        pass_vars: false
      plan:
        skip_planfile: false
      plugin_cache: true
    helmfile:
      base_path: components/helmfile
      use_eks: true
      kubeconfig_path: /dev/shm
      helm_aws_profile_pattern: ""
      cluster_name_pattern: ""
      cluster_name_template: '{{ .vars.namespace }}-{{ .vars.tenant }}-{{ .vars.environment }}-{{ .vars.stage }}-eks-cluster'
      cluster_name: ""
      command: ""
      auto_generate_files: false
    packer:
      base_path: ""
      command: ""
      auto_generate_files: false
    ansible:
      base_path: ""
      command: ""
      auto_generate_files: false
  stacks:
    base_path: stacks
    included_paths:
      - orgs/**/*
    excluded_paths:
      - '**/_defaults.yaml'
    name_pattern: '{tenant}-{environment}-{stage}'
    name_template: ""
  workflows:
    base_path: stacks/workflows
    list:
      format: ""
      columns: []
atmos_component: vpc
atmos_manifest: orgs/acme/plat/dev/us-east-2
atmos_stack: plat-ue2-dev
atmos_stack_file: orgs/acme/plat/dev/us-east-2
