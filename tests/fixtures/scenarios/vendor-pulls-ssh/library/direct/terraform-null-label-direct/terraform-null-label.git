  <span class="pl-v"><span class="pl-smi">tags</span>          <span class="pl-k">=</span> </span>module<span class="pl-k">.</span><span class="pl-smi">eg_prod_bastion_label</span><span class="pl-k">.</span><span class="pl-smi">tags</span>
